{
  "usesTypeScript": true,
  "usesStyledComponents": false,
  "usesCssModule": false,
  "cssPreprocessor": "sass",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "customTemplates": {
        "component": "templates/component/TemplateName.tsx",
        "style": "templates/component/TemplateName.sass"
      },
      "path": "src/components",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    },
    "page": {
      "customTemplates": {
        "component": "templates/page/TemplateName.tsx"
      },
      "path": "src/pages",
      "withStyle": false,
      "withTest": true,
      "withStory": false,
      "withLazy": true
    },
    "section": {
      "customTemplates": {
        "component": "templates/section/TemplateName.tsx",
        "style": "templates/section/TemplateName.sass"
      },
      "path": "src/pages/sections",
      "withStyle": true,
      "withTest": true,
      "withStory": false,
      "withLazy": true
    }
  }
}